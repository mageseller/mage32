<?xml version="1.0" ?>
<form xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
	<argument name="data" xsi:type="array">
		<item name="js_config" xsi:type="array">
			<item name="provider" xsi:type="string">mageseller_xitimport_xitcategory_form.xitcategory_form_data_source</item>
		</item>
		<item name="label" translate="true" xsi:type="string">General Information</item>
		<item name="template" xsi:type="string">templates/form/collapsible</item>
	</argument>
	<settings>
		<buttons>
			<button class="Mageseller\XitImport\Block\Adminhtml\XitCategory\Edit\BackButton" name="back"/>
			<button class="Mageseller\XitImport\Block\Adminhtml\XitCategory\Edit\DeleteButton" name="delete"/>
			<button class="Mageseller\XitImport\Block\Adminhtml\XitCategory\Edit\SaveButton" name="save"/>
			<button class="Mageseller\XitImport\Block\Adminhtml\XitCategory\Edit\SaveAndContinueButton" name="save_and_continue"/>
		</buttons>
		<namespace>mageseller_xitimport_xitcategory_form</namespace>
		<dataScope>data</dataScope>
		<deps>
			<dep>mageseller_xitimport_xitcategory_form.xitcategory_form_data_source</dep>
		</deps>
	</settings>
	<dataSource name="xitcategory_form_data_source">
		<argument name="data" xsi:type="array">
			<item name="js_config" xsi:type="array">
				<item name="component" xsi:type="string">Magento_Ui/js/form/provider</item>
			</item>
		</argument>
		<settings>
			<submitUrl path="*/*/save"/>
		</settings>
		<dataProvider class="Mageseller\XitImport\Model\XitCategory\DataProvider" name="xitcategory_form_data_source">
			<settings>
				<requestFieldName>xitcategory_id</requestFieldName>
				<primaryFieldName>xitcategory_id</primaryFieldName>
			</settings>
		</dataProvider>
	</dataSource>
	<fieldset name="general">
		<settings>
			<label/>
		</settings>
		<field formElement="textarea" name="name" sortOrder="10">
			<argument name="data" xsi:type="array">
				<item name="config" xsi:type="array">
					<item name="source" xsi:type="string">XitCategory</item>
				</item>
			</argument>
			<settings>
				<dataType>text</dataType>
				<label translate="true">name</label>
				<dataScope>name</dataScope>
				<validation>
					<rule name="required-entry" xsi:type="boolean">false</rule>
				</validation>
			</settings>
		</field>
	</fieldset>
</form>
