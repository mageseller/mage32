<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
	<route method="POST" url="/V1/mageseller-xitimport/xitcategory">
		<service class="Mageseller\XitImport\Api\XitCategoryRepositoryInterface" method="save"/>
		<resources>
			<resource ref="Mageseller_XitImport::XitCategory_save"/>
		</resources>
	</route>
	<route method="GET" url="/V1/mageseller-xitimport/xitcategory/search">
		<service class="Mageseller\XitImport\Api\XitCategoryRepositoryInterface" method="getList"/>
		<resources>
			<resource ref="Mageseller_XitImport::XitCategory_view"/>
		</resources>
	</route>
	<route method="GET" url="/V1/mageseller-xitimport/xitcategory/:xitcategoryId">
		<service class="Mageseller\XitImport\Api\XitCategoryRepositoryInterface" method="getById"/>
		<resources>
			<resource ref="Mageseller_XitImport::XitCategory_view"/>
		</resources>
	</route>
	<route method="PUT" url="/V1/mageseller-xitimport/xitcategory/:xitcategoryId">
		<service class="Mageseller\XitImport\Api\XitCategoryRepositoryInterface" method="save"/>
		<resources>
			<resource ref="Mageseller_XitImport::XitCategory_update"/>
		</resources>
	</route>
	<route method="DELETE" url="/V1/mageseller-xitimport/xitcategory/:xitcategoryId">
		<service class="Mageseller\XitImport\Api\XitCategoryRepositoryInterface" method="deleteById"/>
		<resources>
			<resource ref="Mageseller_XitImport::XitCategory_delete"/>
		</resources>
	</route>
</routes>
