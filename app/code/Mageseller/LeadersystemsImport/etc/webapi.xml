<?xml version="1.0" ?>
<routes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Webapi:etc/webapi.xsd">
	<route method="POST" url="/V1/mageseller-leadersystemsimport/leadersystemscategory">
		<service class="Mageseller\LeadersystemsImport\Api\LeadersystemsCategoryRepositoryInterface" method="save"/>
		<resources>
			<resource ref="Mageseller_LeadersystemsImport::LeadersystemsCategory_save"/>
		</resources>
	</route>
	<route method="GET" url="/V1/mageseller-leadersystemsimport/leadersystemscategory/search">
		<service class="Mageseller\LeadersystemsImport\Api\LeadersystemsCategoryRepositoryInterface" method="getList"/>
		<resources>
			<resource ref="Mageseller_LeadersystemsImport::LeadersystemsCategory_view"/>
		</resources>
	</route>
	<route method="GET" url="/V1/mageseller-leadersystemsimport/leadersystemscategory/:leadersystemscategoryId">
		<service class="Mageseller\LeadersystemsImport\Api\LeadersystemsCategoryRepositoryInterface" method="getById"/>
		<resources>
			<resource ref="Mageseller_LeadersystemsImport::LeadersystemsCategory_view"/>
		</resources>
	</route>
	<route method="PUT" url="/V1/mageseller-leadersystemsimport/leadersystemscategory/:leadersystemscategoryId">
		<service class="Mageseller\LeadersystemsImport\Api\LeadersystemsCategoryRepositoryInterface" method="save"/>
		<resources>
			<resource ref="Mageseller_LeadersystemsImport::LeadersystemsCategory_update"/>
		</resources>
	</route>
	<route method="DELETE" url="/V1/mageseller-leadersystemsimport/leadersystemscategory/:leadersystemscategoryId">
		<service class="Mageseller\LeadersystemsImport\Api\LeadersystemsCategoryRepositoryInterface" method="deleteById"/>
		<resources>
			<resource ref="Mageseller_LeadersystemsImport::LeadersystemsCategory_delete"/>
		</resources>
	</route>
</routes>
