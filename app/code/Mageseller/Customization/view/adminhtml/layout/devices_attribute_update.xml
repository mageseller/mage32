<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="admin-2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="Magento\Backend\Block\Store\Switcher" name="devices.store.switcher" as="devices.switcher" template="Magento_Backend::store/switcher.phtml">
                <arguments>
                    <argument name="default_selection_name" xsi:type="string" translate="true">Default Config</argument>
                </arguments>
            </block>
            <block class="Mageseller\Customization\Block\Adminhtml\Attribute\Edit\Devices" name="devices.attribute.html" as="devices"/>
        </referenceContainer>
    </body>
</page>
